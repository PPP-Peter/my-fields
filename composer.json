{
    "name": "pppcreative/price-nice-field",
    "description": "A Laravel Nova nice price field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "pppcreative\\PriceNiceField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "pppcreative\\PriceNiceField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
